<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="200"/>
        </Grid.ColumnDefinitions>
        <ListBox Name="hello"/>
        <StackPanel Grid.Column="1" VerticalAlignment="Center" Margin="15 0">
            <Label Content="С какой страницы парсить:"/>
            <TextBox Margin="0 0 0 7" Name="firstIndex"/>
            <Label Content="По какую страницу парсить:"/>
            <TextBox Margin="0 0 0 7" Name="secondIndex"/>
            <Button Width="120" Height="30" Click="Button_Click">Parse</Button>
        </StackPanel>
        
    </Grid>
</Window>
