<Window x:Class="WpfShow.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfShow"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="100"/>
        </Grid.RowDefinitions>
        <ListBox Name="list"/>
        <StackPanel Grid.Row="1">
            <TextBlock Text="Введите ссылку:"/>
            <TextBox Name="url" Text="https://pkgh.edu.ru/obuchenie/shedule-of-classes.html"/>
            <Button Height="35" Width="100" Content="Парс" Click="Button_Click"/>
        </StackPanel>
    </Grid>
</Window>
